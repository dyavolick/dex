                            ChatbroLoader({ encodedChatId: '02gSj' });
                          </script>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <br><br> <a class="twitter-timeline" data-width="450" data-height="620" href="https://twitter.com/TradexOne?ref_src=twsrc%5Etfw">News by TradexOne</a>
            <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
          </div>

        </div>
      </div>
    </div>
  </div>

</div>

<partial name="_MetamaskModalsPartial" />
<partial name="_TradeModalPartial" model="@tokenSymbol"/>

